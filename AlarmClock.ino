int AlarmArrayContains(Alarm arr_alarm[], Alarm alarm)
{
  for (int i = 0; i < ARR_ALARM_SIZE; i++)
  {
    if ((arr_alarm[i].hours == alarm.hours) && (arr_alarm[i].minutes == alarm.minutes))
    {
      return 1;
    }
  }
  return 0;
}

void PRINT_LCD()
{
  lcd.clear();
  lcd.print(ROW0);
  lcd.setCursor(0,1);
  lcd.print(ROW1);
}

void setup() 
{
  Serial.begin(115200);
  lcd.begin(16, 2);
  rtc.begin();
  pinMode(A2, INPUT);
  pinMode(A3, INPUT);
  pinMode(A0, INPUT);
  pinMode(A1, INPUT);

  arr_Alarm[0] = (Alarm) {-1,-1};
  
  //INIT CUSTOM LCD CHAR
  //lcd.createChar(0, AlarmChar);  

}

void loop() 
{
  t = rtc.getTime();
  PRINT_LCD();
  

  if (!AlarmState)
  {

    if(!AlarmActive && (arr_Alarm[0].hours != -1))
    {
      if ((t.hour != arr_Alarm[0].hours) || (t.min != arr_Alarm[0].minutes))
      {
        AlarmActive = 1;
      }
    }
    
    if ((t.hour == arr_Alarm[0].hours) && (t.min == arr_Alarm[0].minutes) && AlarmActive)
    {
      AlarmState = 1;
      startPlayback(sample, sizeof(sample));
    }
    
    if ((analogRead(A2)==0) && (!Button3Pressed))
    {
      Button3Pressed = 1;
      DeviceState++;
    }
    if ((analogRead(A2)>1))
    {
      Button3Pressed = 0;
    }
    
    if ((analogRead(A3)==0) && (!Button2Pressed) && (DeviceState == SET_CLOCKTIME))
    {
      Button2Pressed = 1;
      SetClockMinutes++;
    }
    if ((analogRead(A3)==0) && (!Button2Pressed) && (DeviceState == ADD_ALARMS))
    {
      Button2Pressed = 1;
      AddAlarmMinutes++;
    }
    if ((analogRead(A3)>1))
    {
      Button2Pressed = 0;
    }
  
    if ((analogRead(A0)==0) && (!Button1Pressed) && (DeviceState == SET_CLOCKTIME))
    {
      Button1Pressed = 1;
      SetClockHours++;
    }
    if ((analogRead(A0)==0) && (!Button1Pressed) && (DeviceState == ADD_ALARMS))
    {
      Button1Pressed = 1;
      AddAlarmHours++;
    }
    if ((analogRead(A0)==0) && (!Button1Pressed) && (DeviceState == DELETE_ALARMS))
    {
      Button1Pressed = 1;
      arr_Alarm[0] = (Alarm) {-1,-1};
      ROW0 = "";
      ROW1 = "ALARM DELETED";
      PRINT_LCD();
      delay(1000);
    }
    if ((analogRead(A0)>1))
    {
      Button1Pressed = 0;
    }
  
  
    if ((analogRead(A1)==0) && (!Button0Pressed) && (DeviceState == SET_CLOCKTIME))
    {
      Button0Pressed = 1;
      rtc.setTime(SetClockHours, SetClockMinutes, 0);
      ROW1 = "TIME SET                           ";
      delay(1000);
    }
    if ((analogRead(A1)==0) && (!Button0Pressed) && (DeviceState == ADD_ALARMS))
    {
      Button0Pressed = 1;
      arr_Alarm[0] = (Alarm) {AddAlarmHours, AddAlarmMinutes};
      ROW1 = "ALARM SET";
      AlarmActive = 1;
      PRINT_LCD();
      delay(1000);
    }
    if ((analogRead(A1)>1))
    {
      Button0Pressed = 0;
    }
  
  
    
    if (DeviceState > 3)
    {
      DeviceState = 0;
    }
    
    switch (DeviceState)
    {
      case PRINT_CLOCK:
        SetClockHours = t.hour;
        SetClockMinutes = t.min;
        ROW0 = String(t.hour) + "h" + String(t.min) + "m" + String(t.sec) + "s";
        ROW1 = "CLOCK                     ";
        break;
      
      case SET_CLOCKTIME:
        SetClockTime();
        ROW0 = String(SetClockHours) + "h" + String(SetClockMinutes) + "m";
        break;
  
      case ADD_ALARMS:
        AddAlarms();
        ROW0 = String(AddAlarmHours) + "h" + String(AddAlarmMinutes) + "m";
        ROW1 = "ADD ALARM                        ";
        break;
  
      case DELETE_ALARMS:
        DeleteAlarms();
        break;
      }
  }
    
  if(AlarmState && AlarmActive)
  {
    ROW0 = "ALARM TRIGGERED!";
    ROW1 = "PRESS ANY BUTTON";
    startPlayback(sample, sizeof(sample));
    for (int i = 0; i<80; i++)
    {
      PRINT_LCD();
      delay(10);
 
      if ((analogRead(A2)==0))
      {
        AlarmState = 0;
        AlarmActive = 0;
        stopPlayback();
        break;
      }
      
    }
  }
}
